<h1>Search Results</h1>

<% if (data == null) { %>
<p>Oops, we could not find the movie! Please refine your search terms.</p>
<% } else { %>
<ol>
    <% data.forEach(movie => { %>
    <li>
        <strong>
            <%= movie.Title %>
        </strong>
        (<%= movie.Year %>)
    </li>
    <img src="<%= movie.Poster %>" alt="movie poster">
    <% }) %>
</ol>
<% } %>
<a href="/">Search Again!</a>